<%= link_to 'All products', products_path %>
<h1><%= @product.title %></h1>

<%= image_tag(@product.image.url, alt: 'Image') if @product.image? %>

<p><%= @product.description %></p>

<p><%= @product.price %></p>

<p><%= @product.category.title %></p>


<p><%= link_to 'Edit', edit_product_path(@product) %></p>
